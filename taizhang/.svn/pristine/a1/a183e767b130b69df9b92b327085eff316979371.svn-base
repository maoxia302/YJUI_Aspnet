
<div style="padding: 15px; overflow: hidden">
    <form id="f_wentichuli_add" method="post">
        <table class="tableForm" width="100%">

            <tr>
                <td colspan="7" style="text-align: center;font-size: 18px;">
                    <b>反馈基本情况</b>
                </td>
            </tr>
            <tr>
                <td style="width: 120px;">
                    编号：
                </td>
                <td id="ID" style="width: 120px;"></td>
                <td style="width: 100px;">
                    反馈时间：
                </td>
                <td id='fkDate' style="width: 120px;"></td>
                <td style="width: 120px;">
                    反馈人：
                </td>
                <td id='fkPerson' style="width: 120px;"></td>
            </tr>
            <tr>
                <td>
                    需支持部门/模块
                </td>
                <td id='supportDep'></td>
                <td>
                    &nbsp;
                </td>
                <td></td>
                <td>
                    &nbsp;
                </td>
                <td></td>
            </tr>
            <tr>
                <td style="width: 120px;">
                    反馈描述：
                </td>
                <td colspan="5" id="fkDesc"></td>
            </tr>
            <tr>
                <td colspan="6" style="text-align: center;font-size: 18px;">
                    <b>问题处理</b>
                </td>
            </tr>
            <tr>
                <td>
                    领取部门：
                </td>
                <td>
                    <input type="text" name="receiveDep" />
                </td>
                <td>
                    领取人：
                </td>
                <td>
                    <input type="text" name="receivePerson" />
                </td>
                <td>
                    &nbsp;
                </td>
                <td>
                    &nbsp;
                </td>
            </tr>
            <tr>
                <td>
                    临时改善:
                </td>
                <td colspan="3"><textarea class="easyui-textbox" cols="50" name="tempGaishan" rows="6"></textarea></td>
                <td>日期：</td>
                <td>
                    <input type="text" name="dyDate" class="Wdate" onfocus="WdatePicker({minDate:'%y-%M-%d'})" />
                </td>
            </tr>

            <tr>
                <td>
                    长期改善:
                </td>
                <td colspan="3"><textarea class="easyui-textbox" cols="50" name="cqFangan" rows="6"></textarea></td>
                <td>日期：</td>
                <td>
                    <input type="text" name="cqDate" class="Wdate" onfocus="WdatePicker({ minDate: '%y-%M-%d' })"/>
                </td>
            </tr>

            <tr>
                <td>
                    完成情况最终描述:
                </td>
                <td colspan="3"><textarea class="easyui-textbox" cols="50" name="endDesc" rows="6"></textarea></td>
                <td></td>
                <td>
                   
                </td>
            </tr>
        </table>
    </form>
</div>
