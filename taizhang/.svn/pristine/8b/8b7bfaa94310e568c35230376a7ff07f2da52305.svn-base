<div style="padding: 15px; overflow: hidden">
    <form id="f_user_edit" method="post">
    <table class="tableForm" width="100%">
        <tr>
            <td>
                账号：
            </td>
            <td>
                <input type="text" name="account" class="easyui-validatebox" data-options="required:true" />
            </td>
            <td>
                密码：
            </td>
            <td>
                <input type="password" name="password" class="easyui-validatebox" data-options="required:true" />
            </td>
            <td>
                确认密码：
            </td>
            <td>
                <input type="password" name="password1" id="password1" class="easyui-validatebox"
                    data-options="required:true, validType:'eqPwd[\'#f_user_edit input[name=password]\']'" />
            </td>
        </tr>
        <tr>
            <td>
                姓名：
            </td>
            <td>
                <input type="text" name="xingming" class="easyui-validatebox" data-options="required:true" />
            </td>
            <td>
                性别：
            </td>
            <td>
                <input type="radio" name="sex" checked="checked" value="男" />
                男
                <input type="radio" name="sex" value="女" />女
            </td>
            <td>
                生日：
            </td>
            <td>
                <input type="text" name="birth" class="easyui-datebox" />
            </td>
        </tr>
        <tr>
            <td>
                身份证：
            </td>
            <td>
                <input type="text" name="sfz" />
            </td>
            <td>
                电话：
            </td>
            <td>
                <input type="text" name="tel" />
            </td>
            <td>
                E-mail：
            </td>
            <td>
                <input type="text" name="email" class="easyui-validatebox" data-options="validType:'email'" />
            </td>
        </tr>
        <tr>
            <td>
                QQ：
            </td>
            <td>
                <input type="text" name="qq" />
            </td>
            <td>
                地址：
            </td>
            <td colspan="3">
                <input type="text" name="dizhi" style="width: 300px" />
            </td>
        </tr>
