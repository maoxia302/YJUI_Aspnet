	padding: 0px;
	line-height: 23px;
	font-weight: bold;
}

.head a
{
	color: White;
	text-decoration: underline;
}

.easyui-accordion ul
{
	list-style-type: none;
	margin: 0px;
	padding: 0px;
}
.easyui-accordion ul li
{
	padding: 0px;
}
.easyui-accordion ul li a
{
	height: 24px;
	padding: 4px;
	line-height: 16px;
	text-decoration: none;
	color: Black;
}
.easyui-accordion ul li div
{
	border: 1px dashed #fff; /*加一个白色的边框 否则hover的时候会跳动*/
	margin: 2px 0px;
	padding-left: 10px;
	padding-top: 5px;
	padding-bottom: 5px;
}
.easyui-accordion ul li div.hover
{
	border: 1px dashed #99BBE8; /*background: #E0ECFF;*/
	background: #fcde61;
	cursor: pointer;
}
.easyui-accordion ul li div.hover a
{
	color: #416AA3;
}
.easyui-accordion ul li div.selected
{
	border: 1px solid #99BBE8;
	background: #E0ECFF;
	cursor: default;
}
.easyui-accordion ul li div.selected a
{
	color: #416AA3;
	font-weight: bold;
}

